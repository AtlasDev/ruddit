{
  "extends": "tslint:latest",
  "rules": {
    "array-type": [ true, "array" ],
    "semicolon": true,
    "quotemark": "single",
    "indent": "tabs",
    "no-console": false,
    "no-empty": false,
		"member-ordering": false,
		"curly": false,
		"align": false,
		"object-literal-sort-keys": false,
		"prefer-for-of": false
  }
}
